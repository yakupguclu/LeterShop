    <link href="~/concept-master/assets/vendor/fonts/fontawesome/css/fontawesome-all.css" rel="stylesheet" />

    <link href="~/concept-master/assets/vendor/charts/chartist-bundle/chartist.css" rel="stylesheet" />

    <link href="~/concept-master/assets/vendor/charts/morris-bundle/morris.css" rel="stylesheet" />

