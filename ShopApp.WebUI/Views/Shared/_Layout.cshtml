    <script src="~/concept-master/assets/vendor/jquery/jquery-3.3.1.min.js"></script>
    <!-- bootstap bundle js -->
    <script src="~/concept-master/assets/vendor/bootstrap/js/bootstrap.bundle.js"></script>
